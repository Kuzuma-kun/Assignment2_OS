    "version": "0.2.0",
    "configurations": [
        {
            "name": "test_sched",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mem",      //
            "args": [
                "input/proc/m0",      //
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "mytask",      //
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}